{
  "name": "com_colintester",
  "version": "0.1.0",
  "description": "Main (onFocus) website for Colin Tester",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Z-omo/com_colintester.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Z-omo/com_colintester/issues"
  },
  "homepage": "https://github.com/Z-omo/com_colintester#readme",
  "scripts": {
    "start": "npm run clearLocal && npm run dev",
    "clearLocal": "rm -rf local/**/*.{css,js,map}",
    "dev": "parcel './src/*.html' --dist-dir local -p 4000",
    "build": "npm run clearWWW && parcel build './src/*.html' --target 'web' && npm run syncResources",
    "clearWWW": "rm -rf www/**/*.{css,js,map}",
    "syncResources": "rsync -a local/resources www/"
  },
  "config": {
    "buildName": "onfocus",
    "srcPath": "./src/",
    "includePath": "./src/html/",
    "devPath": "./local/",
    "buildPath": "./www/"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.19.4",
    "babelify": "^10.0.0",
    "grunt": "^1.5.3",
    "grunt-browserify": "^6.0.0",
    "grunt-contrib-sass": "^2.0.0",
    "grunt-contrib-uglify": "^5.2.2",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-hashres": "^0.4.1",
    "grunt-shell": "^4.0.0",
    "grunt-text-replace": "^0.4.0"
  }
}
